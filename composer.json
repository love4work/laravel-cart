{
    "name": "love4work/laravel-cart",
    "description": "Wrapper for gloudemans/shoppingcart, extend API and improve common methods",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Quentin",
            "email": "peter@love4work.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "gloudemans/shoppingcart": "dev-master",
        "mathiasverraes/money": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0",
        "orchestra/testbench": "~3.3",
        "symfony/var-dumper": "3.1"
    },
    "autoload": {
        "psr-4": {
            "Love4work\\Cart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "": "tests/helpers/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "minimum-stability": "dev"
}
