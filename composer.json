{
    "name": "love4work/laravel-cart",
    "description": "Wrapper for bnet/laravel-cart, improve API readability and improve common methods",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Quentin",
            "email": "peter@love4work.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "bnet/laravel-cart": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0",
        "symfony/var-dumper": "2.7.*@dev"
    },
    "autoload": {
        "psr-4": {
            "Love4Work\\": "src/Love4Work"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "": "tests/helpers/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "minimum-stability": "dev"
}
